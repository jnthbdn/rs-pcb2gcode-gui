<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="FrameInputOutput" parent="GtkBox">
        <property name="orientation">vertical</property>
        <property name="hexpand">true</property>
        <property name="vexpand">true</property>
        <style>
            <class name="main_container"/>
        </style>
        <child>
            <object class="GtkFrame">
                <property name="label" translatable="true">Input files</property>
                <child>
                    <object class="GtkGrid">
                        <property name="row-homogeneous">false</property>
                        <property name="column-spacing">8</property>
                        <property name="row-spacing">4</property>

                        <!-- Front -->
                        <child>
                            <object class="GtkLabel">
                                <property name="label" translatable="true">Front</property>
                                <property name="halign">GTK_ALIGN_START</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="row">0</property>
                                </layout>
                            </object>
                        </child>
                        <child>
                            <object class="BrowseFileObject">
                                <property name="hexpand">true</property>
                                <property name="title">Plop plop</property>
                                <property name="file_pattern_name">
                                    AAA
                                    BBB
                                    CCC
                                </property>
                                <property name="file_pattern">
                                    *.txt
                                    *.grb
                                    *.GRB
                                </property>
                                <layout>
                                    <property name="column">1</property>
                                    <property name="row">0</property>
                                </layout>
                            </object>
                        </child>

                        <!-- Back -->
                        <child>
                            <object class="GtkLabel">
                                <property name="label" translatable="true">Back</property>
                                <property name="halign">GTK_ALIGN_START</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="row">1</property>
                                </layout>
                            </object>
                        </child>
                        <child>
                            <object class="BrowseFileObject">
                                <property name="hexpand">true</property>
                                <layout>
                                    <property name="column">1</property>
                                    <property name="row">1</property>
                                </layout>
                            </object>
                        </child>

                        <!-- Outline -->
                        <child>
                            <object class="GtkLabel">
                                <property name="label" translatable="true">Outline</property>
                                <property name="halign">GTK_ALIGN_START</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="row">2</property>
                                </layout>
                            </object>
                        </child>
                        <child>
                            <object class="BrowseFileObject">
                                <property name="hexpand">true</property>
                                <layout>
                                    <property name="column">1</property>
                                    <property name="row">2</property>
                                </layout>
                            </object>
                        </child>

                        <!-- Drill -->
                        <child>
                            <object class="GtkLabel">
                                <property name="label" translatable="true">Drill</property>
                                <property name="halign">GTK_ALIGN_START</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="row">3</property>
                                </layout>
                            </object>
                        </child>
                        <child>
                            <object class="BrowseFileObject">
                                <property name="hexpand">true</property>
                                <layout>
                                    <property name="column">1</property>
                                    <property name="row">3</property>
                                </layout>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkFrame">
                <property name="label" translatable="true">Pre/Postamble files</property>
                <child>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkFrame">
                <property name="label" translatable="true">Output</property>
                <child>
                </child>
            </object>
        </child>
    </template>
</interface>